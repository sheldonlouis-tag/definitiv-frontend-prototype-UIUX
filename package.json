{
	"name": "definitiv.frontend.uicomponents",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"sync": "svelte-kit sync",
		"fresh": "svelte-kit sync && npm run dev"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^4.1.0",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@internationalized/date": "^3.8.2",
		"@lucide/svelte": "^0.515.0",
		"@playwright/test": "^1.49.1",
		"@storybook/addon-a11y": "^9.1.1",
		"@storybook/addon-docs": "^9.1.1",
		"@storybook/addon-svelte-csf": "^5.0.7",
		"@storybook/addon-themes": "^9.1.1",
		"@storybook/addon-vitest": "^9.1.1",
		"@storybook/sveltekit": "^9.1.1",
		"@sveltejs/adapter-netlify": "^5.2.2",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/adapter-vercel": "^5.10.2",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"@tanstack/table-core": "^8.21.3",
		"@testing-library/jest-dom": "^6.6.4",
		"@testing-library/svelte": "^5.2.8",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^20",
		"@vitest/browser": "^3.2.3",
		"bits-ui": "^2.9.1",
		"clsx": "^2.1.1",
		"embla-carousel-svelte": "^8.6.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-storybook": "^9.1.1",
		"eslint-plugin-svelte": "^3.0.0",
		"formsnap": "^2.0.1",
		"globals": "^16.0.0",
		"layerchart": "^2.0.0-next.27",
		"mode-watcher": "^1.1.0",
		"paneforge": "^1.0.2",
		"playwright": "^1.53.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"publint": "^0.3.2",
		"storybook": "^9.1.1",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-sonner": "^1.0.5",
		"sveltekit-superforms": "^2.27.1",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.0.0",
		"tw-animate-css": "^1.3.6",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vaul-svelte": "^1.0.0-next.7",
		"vite": "^7.0.4",
		"vite-plugin-devtools-json": "^0.2.0",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0"
	},
	"keywords": [
		"svelte"
	],
	"dependencies": {
		"@figma/code-connect": "^1.3.4",
		"lucide-svelte": "^0.536.0"
	}
}
